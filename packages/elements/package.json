{
  "name": "@holochain-open-dev/elements",
  "version": "0.2.4",
  "description": "Common utilities and elements to build Holochain web applications",
  "author": "guillem.cordoba@gmail.com",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holochain-open-dev/common.git"
  },
  "exports": {
    ".": "./dist/index.js",
    "./copiable-hash": "./dist/definitions/copiable-hash.js",
    "./holo-identicon": "./dist/definitions/holo-identicon.js",
    "./display-error": "./dist/definitions/display-error.js",
    "./select-avatar": "./dist/definitions/select-avatar.js"
  },
  "scripts": {
    "start": "vite --open",
