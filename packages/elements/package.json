{
  "name": "@holochain-open-dev/elements",
  "version": "0.0.5",
  "description": "Common utilities and elements to build Holochain web applications",
  "author": "guillem.cordoba@gmail.com",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holochain-open-dev/common.git"
  },
  "exports": {
    ".": "./dist/index.js",
    "./copiable-hash": "./dist/definitions/copiable-hash.js",
    "./holo-identicon": "./dist/definitions/holo-identicon.js",
    "./select-avatar": "./dist/definitions/select-avatar.js"
  },
  "scripts": {
    "analyze": "cem analyze --litelement",
    "start": "tsc && concurrently -k -r \"npm run build:watch\" \"wds\"",
    "build": "tsc && npm run analyze -- --exclude dist",
    "build:watch": "tsc --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@holo-host/identicon": "^0.1.0",
    "@holochain-open-dev/utils": "^0.6.0",
    "@holochain/client": "^0.10.0",
    "@msgpack/msgpack": "^2.7.2",
    "@open-wc/scoped-elements": "2.1.0",
    "@scoped-elements/material-web": "^0.0.19",
    "@scoped-elements/shoelace": "^0.0.10",
    "js-base64": "^3.7.2",
    "lit": "^2.1.3",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.4.17",
    "@rollup/plugin-commonjs": "18.0.0",
    "@types/lodash-es": "^4.17.6",
    "@web/dev-server": "^0.1.34",
    "@web/dev-server-rollup": "^0.3.19",
    "concurrently": "^7.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "customElements": "custom-elements.json"
}
