    "analyze": "cem analyze --litelement"
  },
  "dependencies": {
    "@holo-host/identicon": "^0.1.0",
    "@holochain/client": "^0.12.0",
    "@lit/localize": "^0.11.4",
    "@open-wc/scoped-elements": "2.1.0",
    "@scoped-elements/material-web": "^0.1.1",
    "@scoped-elements/shoelace": "^0.0.10",
    "lit": "^2.6.1"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.4.17",
    "concurrently": "^7.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.0",
    "vite-plugin-checker": "^0.5.5"
  },
  "publishConfig": {
    "access": "public"
